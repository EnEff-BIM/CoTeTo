[GENERATOR]
name = TestMultiOutput
version = 0.1
description = Output the lines of the input files with line numbers added, generate a file with some statistics
author = Jörg Rädler

[LOADER]
name = TextFile
minVer = 0.1
maxVer = 2.0

# any section name that starts with the string TEMPLATE can be used

[TEMPLATE_NUMB]
topFile = numberlines.tmpl
type = mako
ext = .numbered

[TEMPLATE_STAT]
topFile = statistics.tmpl
type = mako
ext = .statistics
